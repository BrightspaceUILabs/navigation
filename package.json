{
  "name": "d2l-navigation",
  "version": "4.8.0",
  "repository": "https://github.com/BrightspaceUI/navigation.git",
  "scripts": {
    "build": "gulp build",
    "lint": "npm run lint:eslint && npm run lint:lit && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer d2l-navigation.js d2l-navigation-separator.js --strict --rules.no-unknown-tag-name off",
    "lint:style": "stylelint \"**/*.{js,html}\"",
    "start": "web-dev-server --node-resolve --watch --open --app-index demo/index.html",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "web-test-runner --config web-test-runner.config.mjs",
    "test:headless:watch": "web-test-runner --config web-test-runner.config.mjs --watch"
  },
  "files": [
    "/components",
    "!/components/lang",
    "d2l-navigation-band.js",
    "d2l-navigation-button-close.js",
    "d2l-navigation-button-notification-icon.js",
    "d2l-navigation-button.js",
    "d2l-navigation-highlight-styles.js",
    "d2l-navigation-immersive.js",
    "d2l-navigation-link-back.js",
    "d2l-navigation-link-image.js",
    "d2l-navigation-link.js",
    "d2l-navigation-main-footer.js",
    "d2l-navigation-main-header.js",
    "d2l-navigation-notification-icon.js",
    "d2l-navigation-separator.js",
    "d2l-navigation-shared-styles.js",
    "d2l-navigation.js",
    "navigation.serge.json"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@brightspace-ui/stylelint-config": "^0.3",
    "@open-wc/testing": "^2",
    "@web/dev-server": "^0.1",
    "@web/test-runner": "^0.13",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-brightspace": "^0.6.4",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1.5.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-ejs": "^3.1.3",
    "gulp-rename": "^1.2.3",
    "lit-analyzer": "^1",
    "merge-stream": "^1.0.1",
    "require-dir": "^1.2.0",
    "stylelint": "^14"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1",
    "@polymer/polymer": "^3",
    "d2l-button": "github:BrightspaceUI/button#semver:^5",
    "d2l-link": "github:BrightspaceUI/link#semver:^5",
    "d2l-localize-behavior": "github:BrightspaceUI/localize-behavior#semver:^2",
    "d2l-polymer-behaviors": "github:Brightspace/d2l-polymer-behaviors-ui#semver:^2",
    "d2l-typography": "github:BrightspaceUI/typography#semver:^7",
    "fastdom": "^1",
    "lit-element": "^2",
    "resize-observer-polyfill": "^1"
  }
}
